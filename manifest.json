{
  "manifest_version": 3,
  "name": "Create a Chrome Extension with TypeScript",
  "version": "1.0.0",
  "action": {},
  "background": {
    "service_worker": "src/background/index.js",
    "type": "module"
  },
  "content_scripts": [  
    {
      "js": ["src/content.js"],
      "matches": ["https://www.google.com/"],
      "type": "module"
    }],
  "permissions": ["scripting", "activeTab"]
}